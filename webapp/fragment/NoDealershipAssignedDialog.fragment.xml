<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		title="{i18n>noDealershipDialogTitle}"
		state="Error"
		class="sapUiPopupWithPadding">
		<content>
			<Text text="{i18n>noDealershipError}"/>
		</content>
		<beginButton>
			<Button text="{i18n>viewMyDealershipButton}" type="Emphasized" press="viewMyDealerships"/>
		</beginButton>
		<endButton>
			<Button text="{i18n>navigateLaunchpadButton}" press="navigateToLaunchpad"/>
		</endButton>		
	</Dialog>
</core:FragmentDefinition>