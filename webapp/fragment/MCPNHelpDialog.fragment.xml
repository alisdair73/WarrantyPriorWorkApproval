<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:vh="sap.ui.comp.valuehelpdialog">
	<SelectDialog 
		noDataText="No Parts Found"	
		title="Select Main Causal Part Number"
		class="sapUiPopupWithPadding"
		items="{/ZDPE_C_MATERIALS}"
		search="onValueHelpSearch"
		liveChange="onValueHelpSearch"
		confirm="onMCPNSelected"
		cancel="onValueHelpClose"
		multiSelect="false">
		<StandardListItem
			title="{materialNo}"
			description="{description}" 
		/>
	</SelectDialog>
</core:FragmentDefinition>